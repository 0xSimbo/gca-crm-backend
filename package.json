{
  "name": "@glowlabs-org/crm-bindings",
  "version": "0.1.0-alpha.129",
  "description": "A library containing all API bindings for the Elysia App",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "build:types": "rm -rf lib && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc  --module es2015 --outDir ./lib/cjs  && node postbuild-cjs.mjs",
    "build:esm": "tsc  --module es2015 --outDir ./lib/esm   && node postbuild-esm.mjs",
    "db:push": "bun test && drizzle-kit push",
    "db:generate": "bun test && drizzle-kit generate",
    "studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.583.0",
    "@elysiajs/bearer": "1.0.2",
    "@elysiajs/cors": "0.8.0",
    "@elysiajs/cron": "1.0.3",
    "@elysiajs/eden": "1.1.2",
    "@elysiajs/swagger": "1.0.5",
    "@glowlabs-org/events-sdk": "^1.0.20",
    "@glowlabs-org/utils": "^0.2.168",
    "@google/generative-ai": "^0.24.1",
    "@neondatabase/serverless": "^0.9.4",
    "@slack/web-api": "^7.10.0",
    "@types/heic-convert": "^2.1.0",
    "decimal.js-light": "^2.5.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.1",
    "elysia": "1.1.1",
    "ethers": "6.15.0",
    "graphql-request": "^7.0.1",
    "heic-convert": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.3.11",
    "nanoid": "^5.0.7",
    "openai": "^6.6.0",
    "p-limit": "^6.2.0",
    "pg": "^8.11.5",
    "postgres": "^3.4.4",
    "redis": "^4.6.13",
    "siwe": "^2.3.2",
    "viem": "^2.9.30",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@types/bun": "^1.1.1",
    "@types/jsonwebtoken": "^9.0.6",
    "bun-types": "latest",
    "drizzle-kit": "^0.21.1"
  },
  "peerDependencies": {
    "@elysiajs/eden": "1.1.2",
    "elysia": "1.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/glowlabs-org/crm-backend.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/cjs/index.js",
  "types": "./lib/esm/index.d.ts",
  "module": "./lib/esm/index.js",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "exports": {
    "types": "./lib/esm/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js"
  },
  "engines": {
    "node": ">=16"
  }
}
