{
  "name": "@glowlabs-org/crm-bindings",
  "version": "0.0.3",
  "description": "A library containing all API bindings for the Elysia App",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "build:types": "rm -rf lib && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc  --module commonjs --outDir ./lib/cjs  && node postbuild-cjs.mjs",
    "build:esm": "tsc  --module es2015 --outDir ./lib/esm   && node postbuild-esm.mjs",
    "db:push": "bun test && drizzle-kit push",
    "db:generate": "bun test && drizzle-kit generate",
    "studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.0.2",
    "@elysiajs/cors": "^0.8.0",
    "@elysiajs/eden": "1.0.12",
    "@elysiajs/swagger": "^1.0.5",
    "@glowlabs-org/guarded-launch-ethers-sdk": "^0.0.3",
    "drizzle-orm": "^0.30.1",
    "elysia": "1.0.16",
    "ethers": "5.7.2",
    "jsonwebtoken": "^9.0.2",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.3.11",
    "nanoid": "^5.0.7",
    "pg": "^8.11.5",
    "postgres": "^3.4.4",
    "redis": "^4.6.13",
    "siwe": "^2.3.2",
    "viem": "^2.9.30"
  },
  "devDependencies": {
    "@types/bun": "^1.1.1",
    "@types/jsonwebtoken": "^9.0.6",
    "bun-types": "latest",
    "drizzle-kit": "^0.21.1"
  },
  "peerDependencies": {
    "@elysiajs/eden": "1.0.12",
    "elysia": "1.0.16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/glowlabs-org/crm-backend.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/cjs/index.js",
  "types": "./lib/esm/index.d.ts",
  "module": "./lib/esm/index.js",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "exports": {
    "types": "./lib/esm/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js"
  },
  "engines": {
    "node": ">=16"
  }
}
